<?xml version="1.0" encoding="UTF-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:orientation="vertical"
    tools:context=".ui.Settings"
    >
    <LinearLayout
        android:id="@+id/linearMenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical">

        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameStacklinedesign"
            android:layout_height="wrap_content"
            android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImageOne"
                android:layout_gravity="top|center"
                android:layout_height="200dp"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_rectangle_49"
                tools:ignore="ContentDescription" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="bottom"
                    android:orientation="horizontal"
                    android:paddingHorizontal="32dp"
                    android:paddingTop="22dp">

                    <TextView
                        android:id="@+id/txtMenu"
                        style="@style/txtRobotoregular30"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:layout_weight="1"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_menu" />

                    <ImageView
                        android:id="@+id/imageShareOne"
                        android:layout_width="@dimen/_30pxh"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_gravity="center"
                        android:layout_weight="0"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_share"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_share" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearProfil"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|center"
                    android:orientation="vertical">

                    <FrameLayout
                        android:id="@+id/frameAvatar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal">

                        <com.google.android.material.imageview.ShapeableImageView
                            android:id="@+id/imageProfile"
                            android:layout_width="@dimen/_120pxh"
                            android:layout_height="@dimen/_120pxh"
                            android:scaleType="centerCrop"
                            tools:ignore="ContentDescription"
                            app:shapeAppearance="@style/photoCorner"
                            android:src="@drawable/profil_vide" />

                        <ImageButton
                            android:id="@+id/btnEdit"
                            android:layout_width="@dimen/_46pxh"
                            android:layout_height="@dimen/_46pxh"
                            android:layout_gravity="bottom|end"
                            android:background="@drawable/rectangle_bg_white_a700_01_border_white_a700_01_radius_23"
                            android:paddingHorizontal="@dimen/_11pxh"
                            android:paddingVertical="@dimen/_11pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_line_design_edit_line"
                            tools:ignore="ContentDescription" />
                    </FrameLayout>

                    <TextView
                        android:id="@+id/username"
                        style="@style/txtPoppinssemibold22"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:lineSpacingExtra="@dimen/_11pxh"
                        android:singleLine="true"
                        android:textSize="@dimen/_24sp"
                        android:text="@string/name" />


                </LinearLayout>
            </LinearLayout>

        </FrameLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/linearContent1"
                    style="@style/groupStylewhite_A700_7"
                    android:elevation="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_24pxh"
                    android:gravity="start"
                    android:orientation="vertical"
                    android:paddingHorizontal="@dimen/_16pxh"
                    android:paddingVertical="@dimen/_16pxv">
                    <LinearLayout android:id="@+id/linearEdit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageReplyOne"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="center"
                            android:src="@drawable/img_reply"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_reply" />

                        <TextView
                            android:id="@+id/btnEditprofile"
                            style="@style/txtRobotoregular16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:textSize="@dimen/_20sp"
                            android:text="@string/msg_edit_profile_information" />
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearNotif"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRownotification"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageImageTwo"
                                android:layout_width="@dimen/_30pxh"
                                android:layout_height="@dimen/_30pxh"
                                android:scaleType="center"
                                android:src="@drawable/img_group"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_group" />

                            <TextView
                                android:id="@+id/txtNotifications"
                                style="@style/txtRobotoregular16"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:ellipsize="end"
                                android:letterSpacing="0.01"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:textSize="@dimen/_20sp"
                                android:text="@string/lbl_notifications" />
                        </LinearLayout>

                        <com.google.android.material.switchmaterial.SwitchMaterial
                            android:id="@+id/switchMaterialToggleNOt"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="end"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearLang"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageLang"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="center"
                            android:src="@drawable/img_group_black_900_02"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtLanguage"
                            style="@style/txtRobotoregular16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|start"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:textSize="@dimen/_20sp"
                            android:text="@string/lbl_language" />
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearTheme"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/linearRowlivephotos"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageLivephotosOne"
                                android:layout_width="@dimen/_30pxh"
                                android:layout_height="@dimen/_30pxh"
                                android:scaleType="centerCrop"
                                android:src="@drawable/img_live_photos"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_live_photos" />

                            <TextView
                                android:id="@+id/txtTheme"
                                style="@style/txtRobotoregular16"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:ellipsize="end"
                                android:letterSpacing="0.01"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:textSize="@dimen/_20sp"
                                android:text="@string/lbl_theme" />
                        </LinearLayout>

                        <com.google.android.material.switchmaterial.SwitchMaterial
                            android:id="@+id/switchMaterialToggleThe"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="end"
                            android:layout_weight="1"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout android:id="@+id/frameContent2"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_marginHorizontal="@dimen/_24pxh"
                    android:layout_width="@dimen/_380pxh"
                    android:orientation="vertical"
                    android:padding="@dimen/_16pxh"
                    android:elevation="@dimen/_8pxh"
                    style="@style/groupStylewhite_A700_7">
                    <LinearLayout android:id="@+id/linearHist"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageHistory"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="centerCrop"
                            android:src="@drawable/history"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtHistory"
                            style="@style/txtRobotoregular16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|start"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:textSize="@dimen/_20sp"
                            android:text="@string/lbl_history" />
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearContact"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageContact"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="centerCrop"
                            android:src="@drawable/contact"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtContactus"
                            style="@style/txtRobotoregular16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|start"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:textSize="@dimen/_20sp"
                            android:text="@string/lbl_contact_us" />
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearPrivacy"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imagePrivacy"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="centerCrop"
                            android:src="@drawable/privacy"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtPrivacy"
                            style="@style/txtRobotoregular16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|start"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:textSize="@dimen/_20sp"
                            android:text="@string/lbl_privacy_policy" />
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearHelp"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageHelp"
                            android:layout_width="@dimen/_30pxh"
                            android:layout_height="@dimen/_30pxh"
                            android:layout_gravity="center_vertical"
                            android:scaleType="centerCrop"
                            android:src="@drawable/help"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtHelp"
                            style="@style/txtRobotoregular16"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|start"
                            android:layout_marginStart="@dimen/_8pxh"
                            android:ellipsize="end"
                            android:letterSpacing="0.01"
                            android:lineSpacingExtra="@dimen/_3pxh"
                            android:singleLine="true"
                            android:textSize="@dimen/_20sp"
                            android:text="@string/lbl_help_support" />

                    </LinearLayout>

                </LinearLayout>

                <FrameLayout android:id="@+id/frameContent3"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginVertical="@dimen/_16pxv"
                    android:layout_width="@dimen/_380pxh"
                    android:padding="@dimen/_16pxh"
                    android:elevation="@dimen/_8pxh"
                    style="@style/groupStylewhite_A700_7">

                    <TextView
                        android:id="@+id/btnLogout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/rectangle_border_white_a700_01_radius_8"
                        android:drawableStart="@drawable/img_user_red_600"
                        android:drawablePadding="@dimen/_8pxh"
                        android:ellipsize="end"
                        android:hint="@string/lbl_logout"
                        android:letterSpacing="0.01"
                        android:lineSpacingExtra="@dimen/_3pxh"
                        android:singleLine="true"
                        android:textColorHint="@color/red_600"
                        android:textSize="20sp"
                        tools:ignore="Autofill" />
                </FrameLayout>

            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</FrameLayout>