<?xml version="1.0" encoding="UTF-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    >

    <FrameLayout android:id="@+id/frameProfil"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        style="@style/groupStyle">
        <LinearLayout android:id="@+id/linearColumnahmedelat"
            android:layout_marginTop="@dimen/_160pxv"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:paddingBottom="@dimen/_4pxv"
            android:paddingTop="@dimen/_63pxv"
            style="@style/groupStylewhite_A700_10">
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_32pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowarrowleft"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/back"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_37pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/back1"
                    tools:ignore="ContentDescription" tools:src="@drawable/back1"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHome"
                    android:layout_gravity="top"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    style="@style/txtRobotoregular30_1"
                    android:text="@string/lbl_home"/>



            </LinearLayout>

            <FrameLayout android:id="@+id/frameStackviewTwo"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content">
                <ImageView android:id="@+id/imageCircleimage"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_150pxh"
                    android:layout_width="@dimen/_150pxh"
                    android:scaleType="centerCrop"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle_18"/>
                <View
                    android:background="@drawable/rectangle_bg_green_a700_radius_12"
                    android:id="@+id/viewViewTwo"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_width="@dimen/_20pxh"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/nom"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:singleLine="true"
                style="@style/txtRobotoregular24"
                android:text="@string/name"/>
            <LinearLayout
                android:gravity="center"
                android:id="@+id/linearRowsubtractOne"
                android:layout_height="wrap_content"
                android:layout_marginVertical="@dimen/_30sp"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_width="match_parent" android:orientation="horizontal">

                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_64pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_subtract"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_subtract"/>
                    <TextView android:ellipsize="end"
                        android:layout_gravity="center_vertical"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        style="@style/txtPoppinsmedium18"
                        android:text="@string/user_role"/>
            </LinearLayout>
            <LinearLayout android:gravity="center"
                android:id="@+id/linearRowview"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtPersonnel"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:singleLine="true"
                        android:background="@drawable/stroke"
                        style="@style/txtPoppinsmedium24_1"
                        android:text="@string/lbl_personnel2"/>


            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumncallOne"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:paddingVertical="@dimen/_12pxv"
                android:paddingHorizontal="@dimen/_16pxh"
                android:elevation="@dimen/_25pxh"
                style="@style/groupStylewhite_A700_7">
                <LinearLayout android:id="@+id/linearRowcallOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <ImageView android:id="@+id/imageCallOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_call_indigo_900_01"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_call_indigo_900_01"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_vertical"
                        android:id="@+id/numTel"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        style="@style/txtPoppinsmedium20_4"
                      />
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowlockOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageLockOne"
                        android:layout_width="@dimen/_30pxh"
                        android:layout_height="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/iconemail"
                        tools:ignore="ContentDescription" />
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEmail"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="false"
                        style="@style/txtPoppinsmedium20_4"
                       />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearRowsettingsOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:gravity="center"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageSettingsOne"
                        android:layout_gravity="center_vertical"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/txtLocation"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="false"
                        style="@style/txtPoppinsmedium20_4"
                       />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearRowcalendarOne"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">
                    <ImageView
                        android:id="@+id/imageCalendarOne"
                        android:layout_gravity="top"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_calendar"
                        tools:ignore="ContentDescription" />
                    <TextView android:ellipsize="end"
                        android:gravity="bottom"
                        android:id="@+id/niveau"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        style="@style/txtPoppinsmedium20_4"
                        />
                    <TextView
                        android:id="@+id/section"
                        android:ellipsize="end"
                        android:gravity="bottom"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        style="@style/txtPoppinsmedium20_4"
                        />
                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content" />
            <LinearLayout android:gravity="center"
                android:id="@+id/linearRowcall"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_width="match_parent" android:orientation="horizontal">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnContact"
                    style="@style/btnSolidRounded_3"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_52pxv"
                    android:layout_gravity="center_vertical"
                    android:drawableStart="@drawable/drawable_img_call_white_a700_01"
                    android:drawablePadding="@dimen/_4pxh"
                    android:elevation="@dimen/_16pxh"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="@dimen/_12pxh"
                    android:paddingStart="@dimen/_25pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_8pxv"
                    android:shadowColor="#00000038"
                    android:shadowDx="25"
                    android:shadowDy="50"
                    android:shadowRadius="70"
                    android:singleLine="true"
                    android:textAllCaps="false"
                    android:text="@string/lbl_contact"
                    />

            </LinearLayout>
            <View
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content" />
            <View
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
